typecoins_list = []
altura_list = []

ayb = input ("jojo")
a, b=map (int, ayb.split())
while a > 0 and b > 0:
    for mesa in range (1):
        for grosor in range (a):  
        typecoins = int(input('ingresa medida'))
        typecoins_list.append(typecoins)
    for altura in range (b):
        altura=int(input('ingresa altura de la pata de la mesa'))
        altura_list.append(altura)


        



        
        